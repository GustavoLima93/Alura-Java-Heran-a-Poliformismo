public class TesteChars {
	
	public static void main(String[] args) {
		
		char letra = 'a';
		System.out.println(letra);
		
		char valor = 99;
		System.out.println(valor);
		
		char conversao = (char)(valor + 1);
		System.out.println(conversao);
		
		String java = "Aprendendo JAVA desde o básico : ";
		String convertendoString = java + 2018;
		
		System.out.println(convertendoString);
	}
	
}
